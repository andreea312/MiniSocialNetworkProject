<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="450.0" stylesheets="@addFriend.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.socialnetworkgradlefx.controller.AddFriendController">
   <children>
      <TableView fx:id="tableView2" layoutX="143.0" prefHeight="400.0" prefWidth="420.0" style="-fx-selection-bar: #ffb3d9; -fx-selection-bar-non-focused: #ffe6f2;">
        <columns>
          <TableColumn fx:id="firstNameColumn2" prefWidth="115.0" text="First Name" />
          <TableColumn fx:id="lastNameColumn2" prefWidth="115.0" text="Last Name" />
            <TableColumn fx:id="emailColumn2" prefWidth="190.0" text="Email" />
        </columns>
      </TableView>
      <Button fx:id="sendFriendshipRequestButton" layoutX="18.0" layoutY="287.0" mnemonicParsing="false" onAction="#sendFriendshipRequestButtonClicked" prefHeight="25.0" prefWidth="108.0" text="Send request" />
      <TextField fx:id="textFieldFirstName2" layoutX="15.0" layoutY="97.0" prefHeight="25.0" prefWidth="115.0" promptText="First name" />
      <TextField fx:id="textFieldLastName2" layoutX="14.0" layoutY="146.0" prefHeight="25.0" prefWidth="115.0" promptText="Last name" />
      <Label fx:id="searchLabel" layoutX="15.0" layoutY="63.0" text="Search users by name" />
      <Button fx:id="searchButton" layoutX="45.0" layoutY="200.0" mnemonicParsing="false" onAction="#searchButtonClicked" text="Search" />
   </children>
</AnchorPane>
