<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="450.0" stylesheets="@sentRequests.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.socialnetworkgradlefx.controller.SentRequestsController">
    <children>
        <TableView fx:id="tableView4" layoutX="143.0" prefHeight="400.0" prefWidth="420.0" style="-fx-selection-bar: #ffb3d9; -fx-selection-bar-non-focused: #ffe6f2;">
            <columns>
                <TableColumn fx:id="firstNameColumn4" prefWidth="110.0" text="First Name" />
                <TableColumn fx:id="lastNameColumn4" prefWidth="110.0" text="Last Name" />
                <TableColumn fx:id="dateColumn4" prefWidth="130.0" text="Date" />
                <TableColumn fx:id="statusColumn4" prefWidth="70.0" text="Status" />
            </columns>
        </TableView>
        <Button fx:id="unsendRequestButton" layoutX="31.0" layoutY="188.0" mnemonicParsing="false" onAction="#unsendRequestButtonClicked" prefHeight="25.0" prefWidth="87.0" text="Unsend" />
      <Label layoutX="14.0" layoutY="6.0" prefHeight="17.0" prefWidth="118.0" text="You sent requests to:" textFill="WHITE" />
    </children>
</AnchorPane>
